	welcomeFrame.E:Destroy()
	wait(.5)
	welcomeFrame:TweenSize(UDim2.new(0,177,0,29), "Out", "Sine", .7)
	wait(.5)
	welcomeFrame:TweenPosition(UDim2.new(7,0,0.387,0), "Out", "Sine", 2.5, true)
	wait(1.2)
	mFrame:TweenPosition(UDim2.new(0,0,0,0), "Out", "Sine", 1)
	
	local Opening = Instance.new("Sound", mFrame)
	Opening.SoundId = "rbxassetid://6914271453"
	Opening.PlaybackSpeed = 1.4
	Opening.Volume = 0.3
	Opening.Looped = false
	Opening:Play()
	
	
end
