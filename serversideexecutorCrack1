local function JIDVEL_fake_script() -- DragBar.Dragify 
	local script = Instance.new('LocalScript', DragBar)

	-- Decompiled with the Synapse X Luau decompiler.
	
	local l__UserInputService__1 = game:GetService("UserInputService");
	function dragify(p1)
		dragToggle = nil;
		dragInput = nil;
		dragStart = nil;
		function updateInput(p2)
			local v1 = p2.Position - dragStart;
			game:GetService("TweenService"):Create(p1, TweenInfo.new(0.21), {
				Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + v1.X, startPos.Y.Scale, startPos.Y.Offset + v1.Y)
			}):Play();
		end;
		p1.InputBegan:Connect(function(p3)
			if p3.UserInputType ~= Enum.UserInputType.MouseButton1 then
				if p3.UserInputType == Enum.UserInputType.Touch then
					if l__UserInputService__1:GetFocusedTextBox() == nil then
						dragToggle = true;
						dragStart = p3.Position;
						startPos = p1.Position;
						p3.Changed:Connect(function()
							if p3.UserInputState == Enum.UserInputState.End then
								dragToggle = false;
							end;
						end);
					end;
				end;
			elseif l__UserInputService__1:GetFocusedTextBox() == nil then
				dragToggle = true;
				dragStart = p3.Position;
				startPos = p1.Position;
				p3.Changed:Connect(function()
					if p3.UserInputState == Enum.UserInputState.End then
						dragToggle = false;
					end;
				end);
			end;
		end);
		p1.InputChanged:Connect(function(p4)
			if p4.UserInputType ~= Enum.UserInputType.MouseMovement then
				if p4.UserInputType == Enum.UserInputType.Touch then
					dragInput = p4;
				end;
			else
				dragInput = p4;
			end;
		end);
		game:GetService("UserInputService").InputChanged:Connect(function(p5)
			if p5 == dragInput then
				if dragToggle then
					updateInput(p5);
				end;
			end;
		end);
	end;
	dragify(script.Parent);
	
end
coroutine.wrap(JIDVEL_fake_script)()
local function TYWDJN_fake_script() -- Source.LocalScript 
	local script = Instance.new('LocalScript', Source)

	local numValue = script.Parent.Value
	local TBX = script.Parent
	
	function colorCode()
		local Text = TBX.Text
		--local newRequireString = "require"
		local convertedNumText = tonumber(Text)
		local convertedStrText = tostring(Text) 
		if Text == convertedStrText then
			TBX.TextColor3 = Color3.fromRGB(129, 237, 248)
			
		if convertedNumText >= 0 then
			TBX.TextColor3 = Color3.fromRGB(231,218,68)
			end
		end
	end
	TBX:GetPropertyChangedSignal("Text"):Connect(colorCode)
end
coroutine.wrap(TYWDJN_fake_script)()
local function TZBDZ_fake_script() -- Execute.RippleEffect 
	local script = Instance.new('LocalScript', Execute)

	-- NOTE: Make sure the ZIndex of the button used is higher than other GUI parts behind
	--       it, or this will not work correctly!
	
	
	
	
	local RippleColor        = Color3.fromRGB(0, 0, 0) -- RGB Color of the ripple effect, you can change it.
	local RippleTransparency = 0.9 ---------------------------- Max is 1, Min is 0
	local PixelSize          = 2000 --------------------------- The Max size of the ripple in pixels
	local TimeLength         = 0.65 ---------------------------- How long the ripple animation is
	local FadeLength         = 0.6 ---------------------------- How long it takes for the ripple to fade out
	
	
	
	-- Do NOT change anything in the script if you want it to work, unless you know how to script...
	
	
	
	-- Script Below
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	local frame = script.Parent
	local rgb = Color3.fromRGB
	local ud2 = UDim2.new
	repeat
		wait()
	until game.Players.LocalPlayer
	local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	
	frame.ClipsDescendants = true
	frame.AutoButtonColor = false
	
	
	function tweenInRipple(ripple)
		spawn(function()
			local TweenService = game:GetService("TweenService")
			local Part = ripple
			local Info = TweenInfo.new(
				TimeLength,
				Enum.EasingStyle.Linear,
				Enum.EasingDirection.InOut,
				0,
				false,
				0
			)
			local Goals = 
			{
				Size = ud2(0, PixelSize, 0, PixelSize);
			}
			local Tween = TweenService:Create(Part, Info, Goals)
			Tween:Play()
		end)
	end
	
	function fadeOutRipple(ripple)
		spawn(function()
			local TweenService = game:GetService("TweenService")
			local Part = ripple
			local Info = TweenInfo.new(
				FadeLength,
				Enum.EasingStyle.Linear,
				Enum.EasingDirection.InOut,
				0,
				false,
				0
			)
			local Goals = 
			{
				ImageTransparency = 1;
			}
			local Tween = TweenService:Create(Part, Info, Goals)
			Tween:Play()
			wait(FadeLength + 0.1)
			ripple:Destroy()
		end)
	end
	
	frame.MouseButton1Down:Connect(function()
		local done = false
		local ripple = script.Circle:Clone()
		ripple.Parent = frame
		ripple.ZIndex = frame.ZIndex + 1
		ripple.ImageColor3 = RippleColor
		ripple.ImageTransparency = RippleTransparency
		tweenInRipple(ripple)
		frame.MouseButton1Up:Connect(function()
			if done == false then
				done = true
				fadeOutRipple(ripple)
			end
		end)
		wait(4);
		done = true;
		fadeOutRipple(ripple) -- if it doesnt detect that it was unselected
	end)
end
coroutine.wrap(TZBDZ_fake_script)()
local function SYPPRN_fake_script() -- Execute.SoundHandler 
	local script = Instance.new('LocalScript', Execute)

	local hoversoundObject = script.Parent.Hover
	local Button = script.Parent
	
	function onHover()
		hoversoundObject:Play()
	end
	
	Button.MouseEnter:Connect(onHover)
end
