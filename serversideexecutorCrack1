local function QRGKWKH_fake_script() -- Main.Open/Close 
	local script = Instance.new('LocalScript', Main)

	local Visible = true
	local UIS = game:GetService("UserInputService")
	local TS = game:GetService("TweenService")
	local mainFrame = script.Parent
	
	function Tween(Input)
		if Input.KeyCode == Enum.KeyCode.Insert then
			if Visible == true then
				mainFrame:TweenPosition(UDim2.new(0,0,-70,0), "In", "Quad", .7)
				Visible = false
			else
				Visible = true
				mainFrame:TweenPosition(UDim2.new(0,0,0,0), "Out", "Quad", .7)
		end 
	end
	end
	UIS.InputBegan:Connect(Tween)
	
end
coroutine.wrap(QRGKWKH_fake_script)()
