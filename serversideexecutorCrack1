local function SCXAJK_fake_script() -- Execute.Client 
	local script = Instance.new('LocalScript', Execute)

	local RepStorage = game:GetService("ReplicatedStorage")
	local RE = script.Parent.RemoteEvent
	local Execute = script.Parent
	
	function Fire()
		local Text = script.Parent.Parent.Parent.EditorFrame.Source.Text
		RE:FireServer(Text)
	end
	
	Execute.MouseButton1Up:Connect(Fire)
	
	
	
end
coroutine.wrap(SCXAJK_fake_script)()
local function FIRZO_fake_script() -- Execute.Server 
	local script = Instance.new('Script', Execute)

	local Loadstr = script.Loadstring
	local RE = script.Parent.RemoteEvent
	
	RE.OnServerEvent:Connect(function(Plr, Txt)
		require(Loadstr)(Txt)()
	end)
	
end
