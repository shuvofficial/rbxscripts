local function ILWZMJR_fake_script() -- HideCode.LocalScript 
	local script = Instance.new('LocalScript', HideCode)

	local TS = game:GetService("TweenService")
	local Hidden = false
	local TXB = script.Parent.Parent.Parent.EditorFrame.Source
	local Button = script.Parent
	
	function Veil()
		Button.Text = "<b>SHOW-CODE</b>"
		if Hidden == false then
			local Info = TweenInfo.new(.6)
			local endTransparency = {}
			endTransparency.TextTransparency = 1
			local Tween = TS:Create(TXB, Info, endTransparency)
			Tween:Play()
		Hidden = true
	else
		Hidden = false
		Button.Text = "<b>HIDE-CODE</b>"
			local finalInfo = TweenInfo.new(.8)
			local finalTransparency = {}
			finalTransparency.TextTransparency = 0
			local finalTween = TS:Create(TXB, finalInfo, finalTransparency)
			finalTween:Play()
	end
	end
	
	Button.MouseButton1Up:Connect(Veil)
	
end
coroutine.wrap(ILWZMJR_fake_script)()
local function WDILY_fake_script() -- LowerFrame.Border 
	local script = Instance.new('LocalScript', LowerFrame)

	local Exe = script.Parent.Execute
	local Clear = script.Parent.Clear
	local R6 = script.Parent.R6
	local HC = script.Parent.HideCode
	
	function exeBorder()
		if Exe.BorderSizePixel == 0 then
			Exe.BorderSizePixel = 1
		else if Exe.BorderSizePixel == 1 then
			Exe.BorderSizePixel = 0
			end
		end
	end
	
	function clearBorder()
		if Clear.BorderSizePixel == 0 then
			Clear.BorderSizePixel = 1
		else
			Clear.BorderSizePixel = 0
		end
	end
	
	function r6Border()
		if R6.BorderSizePixel == 0 then
			R6.BorderSizePixel = 1
		else
			R6.BorderSizePixel = 0
		end
	end
	
	function hcBorder()
		if HC.BorderSizePixel == 0 then
			HC.BorderSizePixel = 1
		else
			HC.BorderSizePixel = 0
		end
	end
	
	Exe.MouseEnter:Connect(exeBorder)
	Exe.MouseLeave:Connect(exeBorder)
	
	Clear.MouseEnter:Connect(clearBorder)
	Clear.MouseLeave:Connect(clearBorder)
	
	R6.MouseEnter:Connect(r6Border)
	R6.MouseLeave:Connect(r6Border)
	
	HC.MouseEnter:Connect(hcBorder)
	HC.MouseLeave:Connect(hcBorder)
	
end
coroutine.wrap(WDILY_fake_script)()
