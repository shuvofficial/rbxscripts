local function JECVSW_fake_script() -- R6_2.Server 
	local script = Instance.new('Script', R6_2)

	local RE = script.Parent.RemoteEvent
	
	
	
	function R6(Plr)
		local R6Char = game.Players:GetCharacterAppearanceAsync(Plr.UserId):Clone()
		local Dummy = script.Parent.DefaultCharacter
		local DummyClone = script.Parent.DefaultCharacter:Clone()
		wait(.05)
		local ogPosition = Plr.Character:WaitForChild("HumanoidRootPart").Position
		local v3Value = script.Parent.V3Value
		v3Value.Value = ogPosition
		local Accessories = {}
		local Extras = {}
		local Hats = {}
	
		for i,v in pairs(Plr.Character:GetChildren()) do
			v:Destroy()
		end
	
		for i,v in pairs(R6Char:GetDescendants()) do
			if v:IsA("CharacterMesh") or v:IsA("Accessory") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("BodyColors") or v:IsA("ShirtGraphic") then
				table.insert(Accessories, v:Clone())
			end
		end
	
		for i,v in pairs(R6Char:GetDescendants()) do
			if v.ClassName == "Decal" and v.Name == "face" then
				DummyClone:WaitForChild("Head").face:Destroy()
				table.insert(Extras, v:Clone())
				if v.ClassName == "Decal" and v.Name == "face" and v.Texture == "rbxasset://textures/face.png" then
					print(":)")
				end
			end 
		end
	
		for i,v in pairs(R6Char:GetDescendants()) do
			if v.ClassName == "SpecialMesh" and v.TextureId == "" then
				table.insert(Extras, v:Clone())
			else
				
			end 
		end
	
		for i,v in pairs(R6Char:GetDescendants()) do
			if v.ClassName == "SpecialMesh" and v.TextureId == "" then
				table.insert(Extras, v:Clone())
			else
				
			end 
		end
		
		for i,v in pairs(R6Char:GetDescendants()) do
			if v.ClassName == "SpecialMesh" and v.TextureId == "" then
				table.insert(Extras, v:Clone())
			else
	
			end 
		end
	
		wait(.05)
		for i,v in pairs(DummyClone:GetChildren()) do
			v.Parent = Plr.Character
		end
		for i,v in pairs(Accessories) do
			v.Parent = Plr.Character
		end
		for i,v in pairs(Extras) do
			v.Parent = Plr.Character.Head
		end
	
	
		wait(.05)
		Plr.Character:WaitForChild("HumanoidRootPart").Position = v3Value.Value
	
	end
	
	
	RE.OnServerEvent:Connect(R6)
	
end
coroutine.wrap(JECVSW_fake_script)()
